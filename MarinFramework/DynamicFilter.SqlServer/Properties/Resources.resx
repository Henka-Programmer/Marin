  <data name="TABLE_METADATA_SQL2019_QUERY" xml:space="preserve">
    <value>SELECT TBL.TABLE_NAME,
       COL.COLUMN_NAME,
       COL.DATA_TYPE,
       COL.IS_NULLABLE,
       PK = CASE
              WHEN CPK.CONSTRAINT_NAME IS NULL THEN 0
              ELSE 1
            END,
       FK = CASE
              WHEN CFK.CONSTRAINT_NAME IS NULL THEN 0
              ELSE 1
            END
FROM   INFORMATION_SCHEMA.TABLES TBL
       INNER JOIN INFORMATION_SCHEMA.COLUMNS COL
               ON COL.TABLE_NAME = TBL.TABLE_NAME
                  AND COL.TABLE_SCHEMA = TBL.TABLE_SCHEMA
       LEFT JOIN SYS.EXTENDED_PROPERTIES PROP
              ON PROP.MAJOR_ID = Object_id(TBL.TABLE_SCHEMA + '.' +
                                           TBL.TABLE_NAME)
                 AND PROP.MINOR_ID = 0
                 AND PROP.NAME = 'MS_Description'
       LEFT JOIN INFORMATION_SCHEMA.TABLE_CONSTRAINTS TPK
               ON TPK.TABLE_NAME = COL.TABLE_NAME
                  AND TPK.TABLE_SCHEMA = COL.TABLE_SCHEMA
                  AND TPK.CONSTRAINT_TYPE = 'PRIMARY KEY'
       LEFT JOIN INFORMATION_SCHEMA.CONSTRAINT_COLUMN_USAGE CPK
              ON CPK.CONSTRAINT_NAME = TPK.CONSTRAINT_NAME
                 AND CPK.COLUMN_NAME = COL.COLUMN_NAME
       LEFT JOIN INFORMATION_SCHEMA.TABLE_CONSTRAINTS TFK
               ON TFK.TABLE_NAME = COL.TABLE_NAME
                  AND TFK.TABLE_SCHEMA = COL.TABLE_SCHEMA
                  AND TFK.CONSTRAINT_TYPE = 'FOREIGN KEY'
       LEFT JOIN INFORMATION_SCHEMA.CONSTRAINT_COLUMN_USAGE CFK
              ON CFK.CONSTRAINT_NAME = TFK.CONSTRAINT_NAME
                 AND CFK.COLUMN_NAME = COL.COLUMN_NAME
  WHERE TBL.TABLE_TYPE = 'base table'</value>
